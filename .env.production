# POS System Production Environment Configuration

# API Configuration
VITE_API_URL=https://api.posapp.com
VITE_API_VERSION=v1
VITE_DISABLE_MOCK=true
VITE_MOCK_DELAY=0

# Feature Flags
VITE_ENABLE_DEBUG_TOOLS=false
VITE_ENABLE_OFFLINE_MODE=true

# Authentication
VITE_AUTH_MODE=oauth
VITE_AUTH_TOKEN_EXPIRY=43200

# Logging
VITE_LOG_LEVEL=error